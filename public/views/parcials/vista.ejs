
<table class="table table-dark table-striped">
  <h1 class="titleProduct">Productos Guardados</h1>
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Price</th>
      <th scope="col">Picture Url</th>
      <th scope="col">Id</th>
    </tr>
  </thead>
  <tbody>
    <% if (products.length > 0) { %>
      <% products.forEach(products=> { %>
        <tr>
          <th scope="row">1</th>
          <td><%= products.title %></td>
          <td><%= products.price %></td>
          <td><img src="<%= products.pictureUrl %>"></td>
          <td><%= products.id %></td>
        </tr>
        <% }) %>
    <% } else {%>
      <h1>No hay Informacion</h1>
    <% } %>
  </tbody>
</table>